{
	"name": "discord-xplus",
	"version": "1.1.1",
	"description": "LevelManager for Discord.js using MongoDB as database, also handle some events for best experience",
	"main": "./src/index.js",
	"types": "./typings/index.d.ts",
	"scripts": {
		"start": "node ./sss/index.js",
		"dev": "nodemon ./sss/index.js",
		"doc": "jsdoc -c jsdoc.json --verbose",
		"dedoc": "gh-pages -d doc",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"prepare": "husky install"
	},
	"files": [
		"src",
		"typings"
	],
	"keywords": [
		"discord",
		"xp",
		"level",
		"manager",
		"mongodb",
		"node"
	],
	"author": "IkarosKurtz",
	"license": "MIT",
	"devDependencies": {
		"@commitlint/cli": "^17.4.4",
		"@commitlint/config-conventional": "^17.4.4",
		"gh-pages": "4.0.0",
		"husky": "^8.0.3",
		"jsdoc": "4.0.0",
		"nodemon": "2.0.20",
		"prettier": "2.8.1",
		"standard": "^17.0.0",
		"vitest": "0.26.3"
	},
	"dependencies": {
		"discord.js": "14.7.1",
		"dotenv": "16.0.3",
		"lodash": "^4.17.21",
		"mongoose": "6.8.0"
	},
	"eslintConfig": {
		"extends": "./node_modules/standard/eslintrc.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/IkarosKurtz/discord-xplus.git"
	},
	"bugs": {
		"url": "https://github.com/IkarosKurtz/discord-xplus/issues"
	},
	"homepage": "https://github.com/IkarosKurtz/discord-xplus#readme"
}
